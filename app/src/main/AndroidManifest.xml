<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ztkx.petagram">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/dogfootprintfilled"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VisualizarMascotas"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MascotasFavoritas"
            android:parentActivityName=".VisualizarMascotas" />
        <activity android:name=".Contacto" />
        <activity android:name=".AcercaDe"></activity>
    </application>

</manifest>